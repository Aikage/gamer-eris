{
  "name": "gamer-eris",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "tsc --noEmit && eslint '*/**/*.ts'",
    "lint": "tsc --noEmit && eslint '*/**/*.ts' --quiet --fix",
    "start": "node production/src/index",
    "build": "tsc",
    "dev": "tsc --watch",
    "develop": "npm run build && npm run start",
    "clean": "rm -r -f node_modules || true && rm -r -f production || true && rm yarn.lock -f -r || true && rm package-lock.json -f -r || true && npm i",
    "reset": "npm run clean && npm run develop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skillz4Killz/gamer-eris.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Skillz4Killz/gamer-eris/issues"
  },
  "homepage": "https://github.com/Skillz4Killz/gamer-eris#readme",
  "dependencies": {
    "@types/node-fetch": "^2.5.2",
    "canvas": "^2.6.0",
    "canvas-constructor": "^3.1.0",
    "chalk": "^2.4.2",
    "confusables": "^0.3.3",
    "eris": "^0.10.1",
    "get-urls": "^9.2.0",
    "i18next": "^17.3.0",
    "i18next-node-fs-backend": "^2.1.3",
    "mongoose": "^5.7.6",
    "node-fetch": "^2.6.0",
    "typescript": "^3.7.2",
    "yuuko": "^1.0.1"
  },
  "devDependencies": {
    "@types/i18next-node-fs-backend": "^2.1.0",
    "@types/mongoose": "^5.5.23",
    "@types/node": "^12.11.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "github:prettier/prettier"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
